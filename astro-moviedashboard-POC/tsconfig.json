{
"compilerOptions": {
"target": "ES2020",
"module": "ESNext",
"lib": ["DOM", "ES2020"],
"jsx": "react-jsx",
"jsxImportSource": "preact",
"moduleResolution": "Node",
"strict": true,
"esModuleInterop": true,
"skipLibCheck": true,
"forceConsistentCasingInFileNames": true,
"allowJs": false,
"noEmit": true
},
"include": ["src"],
"exclude": ["node_modules"]
}